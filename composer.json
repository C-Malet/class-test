{
    "name": "c-malet/class-test",
    "description": "PHP Class testing",
    "keywords": [
        "PHPUnit",
        "Prophecy",
        "Test",
        "Mock"
    ],
    "homepage": "https://github.com/c-malet/class-test",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Clement Malet",
            "email": "malet.pro@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "ClassTest\\": "src/",
            "ClassTest\\Tests\\": "tests/"
        }
    },

    "require": {
        "php": "^8.1",
        "phpunit/phpunit": "^9.0||^10.0",
        "phpspec/prophecy": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.41.1",
        "phpstan/phpstan": "^1.10.50",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpcov": "^9.0.2"
    }
}
